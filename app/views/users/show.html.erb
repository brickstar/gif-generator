<h1>Welcome, <%= current_user.username %>!</h1>
<% if current_user.admin? %>
  <%= link_to 'Generate Gif', new_admin_gif_path %>
  <%= link_to 'Log out', root_path %>
<% else %>
  <%= link_to 'Log out', root_path %>
<% end %>
<% @gifs.each do |gif| %>
  <%= gif.category.name %>
  <img src="<%=gif.image_path%>" alt='picture'>
<% end %>
